{
    "extends": [
        "react-app",
        "react-app/jest",
        "airbnb", 
        "prettier"
    ],
    "rules" : {
        "semi" : "warn",
        "no-var" : 1,
        "prefer-const": "error",
        "react/react-in-jsx-scope":"off",
        "react/jsx-filename-extension":[1,{"extensions" :[".js",".jsx"] }],
        "react/function-component-definition" : "off"

        
    }
}